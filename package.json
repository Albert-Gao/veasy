{
    "name": "easyv",
    "version": "0.1.0",
    "author": "Albert Gao",
    "main": "dist/developer.js",
    "description": "A NPM package ES6 boilerplate ready to use .",
    "contributors": [
        {
            "name": "Albert Gao",
            "email": "albertgaohy@gmail.com"
        }
    ],
    "scripts": {
        "lint": "eslint src test",
        "clean": "rimraf dist/",
        "build": "rimraf dist/ && babel src/ --out-dir dist/",
        "test": "./node_modules/.bin/jest || true",
        "example": "node example.js",
        "prepublish": "npm run clean && npm run lint && npm run test && npm run build"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Mr-Binary/es6-npm-boilerplate.git"
    },
    "keywords": [
        "module",
        "es6",
        "es5",
        "npm",
        "babel"
    ],
    "devDependencies": {
        "babel-cli": "6.26.0",
        "babel-core": "6.26.0",
        "babel-eslint": "8.0.1",
        "babel-plugin-transform-class-properties": "6.24.1",
        "babel-plugin-transform-object-rest-spread": "^6.26.0",
        "babel-preset-env": "1.6.1",
        "babel-preset-react": "6.24.1",
        "enzyme": "3.1.0",
        "enzyme-adapter-react-16": "1.0.2",
        "enzyme-to-json": "3.1.4",
        "eslint": "4.9.0",
        "eslint-config-airbnb": "16.1.0",
        "eslint-config-prettier": "^2.6.0",
        "eslint-plugin-import": "2.7.0",
        "eslint-plugin-jsx-a11y": "6.0.2",
        "eslint-plugin-react": "7.4.0",
        "jest": "^21.2.1",
        "react": "16.0.0",
        "react-dom": "16.0.0",
        "react-test-renderer": "16.0.0",
        "rimraf": "2.6.2",
        "run-sequence": "2.2.0"
    },
    "engine": "node >= 6.5.x",
    "homepage": "https://github.com/Mr-Binary/es6-npm-boilerplate#readme",
    "bugs": {
        "url": "https://github.com/Mr-Binary/es6-npm-boilerplate/issues"
    },
    "license": "MIT",
    "prettier": {
        "singleQuote": true,
        "tabWidth": 2
    },
    "dependencies": {
        "is_js": "0.9.0",
        "prop-types": "15.6.0"
    },
    "jest": {
        "snapshotSerializers": [
            "enzyme-to-json/serializer"
        ]
    }
}
